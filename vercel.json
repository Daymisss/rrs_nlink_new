{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "functions/send-booking-notification/index.js",
      "use": "@vercel/edge"
    },
    {
      "src": "functions/send-calendar-event/index.js",
      "use": "@vercel/edge"
    }
  ],
  "routes": [
    {
      "src": "/api/send-booking-notification",
      "dest": "/functions/send-booking-notification/index.js"
    },
    {
      "src": "/api/send-calendar-event",
      "dest": "/functions/send-calendar-event/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
